// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./momentazos.db"
}

model Momentazo {
  id	      Int     @id @default(autoincrement())
  titulo      String
  descripcion String?
  publicador  String
  url         String
  fecha	      String
}

